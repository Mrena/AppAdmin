<?xml version="1.0" encoding="utf-8"?>
<widget xmlns="http://www.w3.org/ns/widgets"
        xmlns:rim="http://www.blackberry.com/ns/widgets"
        version="1.0.0.0" id="AppAdmin">
 
  <author href="http://mrena.neq3.com/"
          rim:copyright="Copyright 2012-2013 Mrena Systems">Mrena Systems</author>
 
  <name>AppAdmin</name>
 
  <description>Application management on the go.</description>
  <icon src="images/code_icon-150.png"/>
 
  <rim:splash src="images/code_splash-1280x768.png"/>
  <rim:splash src="images/code_splash-768x1280.png"/>
 
  <content src="index.html" rim:allowInvokeParams="true"/>
 
  <feature id="blackberry.ui.dialog"/>
   
  <feature id="blackberry.app">
    <param name="mode" value="portrait" />
  </feature>
 
  <feature id="blackberry.app">
    <param name="backgroundColor" value="0xFFFF0000" />
  </feature>
  <access uri="*" subdomains="true"></access>
  <license href="bin/licence.txt"/>
 
</widget>